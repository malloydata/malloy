{
  "name": "@malloydata/malloy-tag",
  "version": "0.0.339",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "!dist/**/*.spec.*",
    "src",
    "!src/**/*.spec.*"
  ],
  "homepage": "https://github.com/malloydata/malloy#readme",
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/malloydata/malloy"
  },
  "scripts": {
    "test": "jest --config=../../jest.config.js",
    "clean": "rm -rf dist && tsc --build --clean && rm -f tsconfig.tsbuildinfo && rimraf @flowtyped",
    "build": "tsc --build && npm run generate-flow",
    "prepublishOnly": "npm run clean && npm run build",
    "generate-flow": "ts-node ../../scripts/gen-flow.ts"
  },
  "dependencies": {
    "assert": "^2.0.0",
    "@malloydata/motly-ts-parser": "^0.0.1"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2"
  }

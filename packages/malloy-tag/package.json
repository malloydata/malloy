{
  "name": "@malloydata/malloy-tag",
  "version": "0.0.294",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/malloydata/malloy#readme",
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/malloydata/malloy"
  },
  "scripts": {
    "test": "jest --config=../../jest.config.js",
    "build-parser": "mkdir -p src/lib && nearleyc src/MalloyTag.ne --out src/lib/MalloyTagNew.ts",
    "clean": "tsc --build --clean && rimraf @flowtyped",
    "build": "npm run build-parser && tsc --build && npm run generate-flow",
    "prepublishOnly": "npm run clean && npm run build",
    "generate-flow": "ts-node ../../scripts/gen-flow.ts"
  },
  "dependencies": {
    "assert": "^2.0.0",
    "uuid": "^8.3.2",
    "moo": "^0.5.2",
    "nearley": "^2.20.1"
  },
  "devDependencies": {
    "@types/moo": "^0.5.10",
    "@types/nearley": "^2.11.5",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2"
  }
}

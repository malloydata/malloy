{
  "name": "@malloydata/malloy",
  "version": "0.0.1",
  "license": "GPL-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --config=../../jest.config.js",
    "build-parser": "cd src/lang/grammar && cross-os build-parser",
    "testLang": "yarn test --testPathPattern=packages/malloy/src/lang",
    "clean": "rm -rf src/lang/lib",
    "build": "yarn build-parser && yarn tsc --build",
    "malloyc": "ts-node ../../scripts/malloy-to-json"
  },
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.4",
    "assert": "^2.0.0",
    "blueimp-md5": "^2.19.0",
    "lodash": "^4.17.20",
    "luxon": "^1.26.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.165",
    "@types/luxon": "^1.26.4",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "path": "^0.12.7"
  },
  "cross-os": {
    "build-parser": {
      "darwin": "build-parser.sh)",
      "linux": "build-parser.sh)",
      "win32": "build-parser.cmd"
    }
  }
}

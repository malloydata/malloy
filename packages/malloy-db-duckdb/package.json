{
  "name": "@malloydata/db-duckdb",
  "version": "0.0.339",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=20"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./wasm": {
      "browser": "./dist/duckdb_wasm_connection_browser.js",
      "node": "./dist/duckdb_wasm_connection_node.js",
      "types": "./dist/duckdb_wasm_connection_browser.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "./dist/index.d.ts"
      ],
      "wasm": [
        "./dist/duckdb_wasm_connection_browser.d.ts"
      ]
    }
  },
  "homepage": "https://github.com/malloydata/malloy#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/malloydata/malloy"
  },
  "scripts": {
    "lint": "eslint '**/*.ts{,x}'",
    "lint-fix": "eslint '**/*.ts{,x}' --fix",
    "test": "jest --config=../../jest.config.js",
    "build": "tsc --build",
    "clean": "tsc --build --clean && rm -f tsconfig.tsbuildinfo",
    "malloyc": "ts-node ../../scripts/malloy-to-json",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@duckdb/duckdb-wasm": "1.33.1-dev13.0",
    "@duckdb/node-api": "1.4.3-r.1",
    "@malloydata/malloy": "0.0.339",
    "@motherduck/wasm-client": "^0.6.6",
    "apache-arrow": "^17.0.0",
    "web-worker": "^1.3.0"
  }
}
